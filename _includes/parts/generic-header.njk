<header class="page-header">
	{% if title %}
		<h2 class="page-title">{{ title }}</h2>
	{% else %}
		<h2 class="page-title">{{ strain }}</h2>
		<h3 class="price-level">{{ priceLevel }}
	{% endif %}
	{% if desc %}
	<p class="desc">{{ desc | safe }}</p>
	{% endif %}
	{% if tip %}
	<p class="tip">{{ tip | safe }}</p>
	{% endif %}
</header>
